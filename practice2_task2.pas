program PowerOfTen;

var
  A, A2, A4, A5, A10: real;

begin
  // Вводим число A
  writeln('Введите число A:');
  readln(A);

  // Возводим A в квадрат
  A2 := A * A;

  // Возводим A2 в квадрат еще раз
  A4 := A2 * A2;

  // Возводим A4 в квадрат еще раз и умножаем на A
  A5 := A4 * A;

  // Получаем A^10 умножив A5 на A5
  A10 := A5 * A5;

  // Выводим результат
  writeln('A^10 = ', A10);
end.
